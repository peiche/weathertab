{
	"manifest_version": 2,
	"name": "WeatherTab [GH]",
	"short_name": "WeatherTab",
	"version": "2.0.0",
	"description": "A beautiful forecast with every new tab.",
	"author": "Paul Eiche",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["geolocation", "storage"],
	"icons": {
		"48": "dist/img/icon48.png",
		"128": "dist/img/icon128.png"
	}
}
