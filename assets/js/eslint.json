{
	"extends": "eslint:recommended",
	"env": {
		"es6": true,
		"browser": true,
		"jquery": true
	},
	"globals": {
		"chrome": true,
		"Materialize": true,
		"WeatherTab": true,
		"ScrollReveal": true
	},
	"rules": {
		"no-alert": 2,
		"no-console": 0,
		"curly": 2,
		"semi": 2,
		"quotes": [
			"error",
			"single", {
				"allowTemplateLiterals": true
			}
		]
	}
}
